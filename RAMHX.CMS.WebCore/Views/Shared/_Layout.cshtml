    <section class="no-padding-tb">
        <div class="rev_slider_wrapper">
            <!-- START REVOLUTION SLIDER 5.0 auto mode -->
            <div class="rev_slider slider-home-2" data-version="5.0" id="slider-h2">
                <ul>
                    @foreach (var slide in sliders)
                    {
                        <li data-transition="incube-horizontal">
                            <img alt="" data-bgfit="cover" data-bgposition="center center" src="@slide.SliderImagePath" />
                            <div class="tp-caption heading-2 white-text" data-start="700" data-transform_in="y:[-150%];z:0;rX:0deg;rY:0;rZ:0;sX:1;sY:1;skX:0;skY:0;opacity:0;s:600;e:Power3.easeInOut;" data-voffset="-100" data-x="center" data-y="center"><strong>@slide.FirstLine</strong></div>

                            <div class="tp-caption heading-7 white-text" data-start="900" data-transform_in="y:[150%];z:0;rX:0deg;rY:0;rZ:0;sX:1;sY:1;skX:0;skY:0;opacity:0;s:600;e:Power3.easeInOut;" data-voffset="-45" data-x="center" data-y="center">@slide.SecondLine</div>

                            <div class="tp-caption heading-2 white-text text-center" data-start="1100" data-transform_in="y:[150%];z:0;rX:0deg;rY:0;rZ:0;sX:1;sY:1;skX:0;skY:0;opacity:0;s:600;e:Power3.easeInOut;" data-voffset="40" data-x="center" data-y="center">
                                @slide.ThirdLine<br />
                                @slide.ForthLine
                            </div>

                            @if (slide.FirstButtonText != "" || slide.FirstButtonLink != "")
                            {
                                <div class="tp-caption btn-1" data-hoffset="-85" data-start="1600" data-transform_out="y:50px;opacity:0;s:300;" data-voffset="140" data-x="center" data-y="center">
                                    <a class="ot-btn btn-main-color text-cap " href="@slide.FirstButtonLink">@slide.FirstButtonText</a>
                                </div>
                            }

                            @if (slide.SecondButtonText != "" || slide.SecondButtonLink != "")
                            {
                                <div class="tp-caption btn-2" data-hoffset="85" data-start="1600" data-transform_in="y:50px;opacity:0;s:800;e:easeInOutCubic;" data-transform_out="y:50px;opacity:0;s:300;" data-voffset="140" data-x="center" data-y="center"><a class="ot-btn btn-sub-color text-cap  " href="@slide.SecondButtonLink">@slide.SecondButtonText</a></div>
                            }
                        </li>
                    }
                </ul>
            </div>
            <!-- END REVOLUTION SLIDER -->
        </div>
