@{
    Layout = null;
}

<section class="content-header">
    <h1>
        Send Quick SMS
    </h1>

    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#"> Send Quick SMS</a></li>
    </ol>
</section>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <div class="col-lg-12">
                        <label class="control-label">Numbers</label>
                    </div>
                    <div class="col-lg-12">
                        <textarea name="text" cols="80" rows="5" id="txtNumber"></textarea>
                    </div>
                    <div class="col-lg-12">
                        <label class="control-label">Message</label>
                    </div>
                    <div class="col-lg-12">
                        <textarea name="text" cols="80" rows="5" id="txtMessage"></textarea>
                    </div>

                    <div class="col-lg-12">
                        <div class="form-group actionBar">
                            <input class="btn crud-submit btn-success" type="button" id="btnSendSMS" value="Send SMS" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/distadmin/js/AppAdmin/Masters/SendQuickSMS.js?_=@DateTime.Now.Ticks"></script>