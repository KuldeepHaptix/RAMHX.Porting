@{
    Page.Title = "Title goes here";
    Layout = null;

}

<section class="content-header">
    <h1>
        Testimonial Category
        <small>Maintain Testimonial</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Masters</a></li>
        <li><a href="#">Testimonial Category</a></li>
    </ol>
</section>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <div class="">
                        <button type="button" id="btnAddNew" class="btn btn-link">
                            Add New Testimonial
                        </button>
                    </div>
                    <div>&nbsp;</div>
                    <div id='jqxWidget'>
                        <div id="jqxgrid"></div>
                        <div id="popupWindow" class="hide">
                            <div>Testimonial Category Details - Edit</div>
                            <div style="overflow: auto;">
                                <div class="callout callout-danger hide errorDiv">
                                    <h4> Please Correct Below Error(s)</h4>
                                    <p id="perror"></p>
                                </div>
                                <div class="form-group">
                                    <div id="dTestimonialCat">
                                        <label class="control-label">Testimonial Category</label>
                                        <input type="text" name="Testimonial" class="form-control" id="txtTestimonialCatName" isrequired="1" data-error="Please enter Testimonial Category." errorspan="sTestimonialCat" divcontainer="dTestimonialCat" defaultvalue="" />
                                        <span class="help-block hide error" id="sTestimonialCat">Please Enter Category</span>
                                    </div>
                                </div>
                                <div class="form-group actionBar">
                                    <input class="btn crud-submit btn-success" type="button" id="btnSave" value="Save" />
                                    <input id="btnCancel" class="btn crud-submit btn-danger" type="button" value="Cancel" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<script src="~/distadmin/js/AppAdmin/Masters/TestimonialCategory.js?_=@DateTime.Now.Ticks"></script>