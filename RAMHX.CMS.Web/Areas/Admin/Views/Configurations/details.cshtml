@model RAMHX.CMS.DataAccess.app_Configs
@{
    ViewBag.Title = "details";
}
<section class="content-header">
    <h1>
        Configurations
        <small>Maintain Configurations</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Administration</a></li>
        <li><a href="#">Configurations</a></li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <div>
                        <dl class="dl-horizontal">
                            <dt>
                                #Group Id
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.GroupId)
                            </dd>

                            <dt>
                                #Item Id
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ItemId)
                            </dd>

                            <dt>
                                Name
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ItemName)
                            </dd>
                            <dt>
                                Description
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ItemDesc)
                            </dd>
                            <dt>
                                Active?
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.IsActive)
                            </dd>
                            <dt>
                                Short Description
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ShortDesc)
                            </dd>
                            <dt>
                                Code
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.ItemCode)
                            </dd>
                            @*<dt>
                                    HTML Preview
                                </dt>

                                <dd>
                                    @Html.Raw(Model.HtmlModuleHTML)
                                </dd>*@
                        </dl>
                    </div>
                    <p>
                        @Html.ActionLink("Edit", "Edit", new { gid = Model.GroupId, itmid = Model.ItemId }) |
                        @Html.ActionLink("Back to List", "Index")
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>
