@{
    //RolePrincipal r = (RolePrincipal)User;
    string roles = string.Join(",", SiteContext.CurrentUser_Roles.Select(x => x.Name));
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>RAMHX CMS - @ViewBag.Title</title>

    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">

    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/daterangepicker/daterangepicker.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/datepicker/datepicker3.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/iCheck/all.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/colorpicker/bootstrap-colorpicker.min.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/timepicker/bootstrap-timepicker.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/colorpicker/bootstrap-colorpicker.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/dist/css/skins/_all-skins.min.css">
    <link rel="stylesheet" href="/Assets/toastr/toastr.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="/Areas/Admin/Theme/AdminLTE/plugins/select2/select2.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="/Assets/css/custom.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/myadmin" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>My</b>D</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>MyAdmin</b>Doctor</span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="/Areas/Admin/Theme/AdminLTE/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">@SiteContext.CurrentUser_Name</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="/Areas/Admin/Theme/AdminLTE/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                                    <p>
                                        @SiteContext.CurrentUser_Name
                                        <small>@roles</small>
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <ul id="nav" class="sidebar-menu">
                    <li class="header">DOCTOR</li>
                    <li><a href="/myadmin/availability"><i class="fa fa-book"></i> <span>My Availability</span></a></li>
                    <li class="header">COMMAN</li>
                    <li><a href="/myadmin/pos"><i class="fa fa-circle-o text-yellow"></i> <span>POS</span></a></li>
                    <li><a href="/myadmin/location"><i class="fa fa-circle-o text-red"></i> <span>Locations</span></a></li>
                   
                    <li><a href="#"><i class="fa fa-circle-o text-aqua"></i> <span>Information</span></a></li>
                    <li><a href="/myadmin/services"><i class="fa fa-circle-o text-aqua"></i> <span>Services</span></a></li>
                    <li><a href="/myadmin/Holiday"><i class="fa fa-circle-o text-aqua"></i> <span>Holiday</span></a></li>

                    @Html.Partial("~/Views/Shared/LocationNavigation.cshtml")

                    <li><a href="/myadmin/bookappointment"><i class="fa fa-circle-o text-aqua"></i> <span>Book Appointment</span></a></li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.7
            </div>
            <strong>Copyright &copy; 2014-2016 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights
            reserved.
        </footer>
        <div class="control-sidebar-bg"></div>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.3 -->
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.blockUI/2.70/jquery.blockUI.js"></script>
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/jQueryUI/jquery-ui.min.js"></script>
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/jQueryUI/jquery.ui.touch-punch.min.js"></script>

    <!-- Bootstrap 3.3.6 -->
    <script src="/Areas/Admin/Theme/AdminLTE/bootstrap/js/bootstrap.min.js"></script>

    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/input-mask/jquery.inputmask.js"></script>
        <script src="/Areas/Admin/Theme/AdminLTE/plugins/input-mask/jquery.inputmask.date.extensions.js"></script>
        <script src="/Areas/Admin/Theme/AdminLTE/plugins/input-mask/jquery.inputmask.extensions.js"></script>*@

    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>

    <script src="/Areas/Admin/Theme/AdminLTE/plugins/datepicker/bootstrap-datepicker.js"></script>

    <!-- Select2 -->
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/select2/select2.full.min.js"></script>
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/timepicker/bootstrap-timepicker.min.js"></script>

    <script src="/Assets/js/Comman.js"></script>
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="/Areas/Admin/Theme/AdminLTE/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="/Areas/Admin/Theme/AdminLTE/dist/js/app.min.js"></script>
    <!-- Sparkline -->
    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/sparkline/jquery.sparkline.min.js"></script>*@
    <!-- jvectormap -->
    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>*@
    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>*@
    <!-- SlimScroll 1.3.0 -->
    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js"></script>*@

    <!-- ChartJS 1.0.1 -->
    @*<script src="/Areas/Admin/Theme/AdminLTE/plugins/chartjs/Chart.min.js"></script>*@
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @* <script src="/Areas/Admin/Theme/AdminLTE/dist/js/pages/dashboard2.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    <script src="/Areas/Admin/Theme/AdminLTE/dist/js/demo.js"></script>
    <script src="/Assets/toastr/toastr.min.js"></script>

    <script src="/Assets/js/Avaibility.js"></script>
    <script src="/Assets/js/location.js"></script>
    <script src="/Assets/js/Service.js"></script>
    <script src="/Assets/js/Holiday.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
    <script src="/Assets/js/pos.js"></script>
    <script src="/Assets/js/jquery.mcautocomplete.js"></script>
    <script src="/Assets/js/AppointmentByStaff.js"></script>

    @RenderSection("scripts", required: false)

    <script type="text/javascript">
        var current = location.pathname;
        if (location.search != null && location.search != undefined)
        {
            current = location.pathname + location.search
        }

        $(".select2").select2();

        $('#nav li a').each(function () {
            var $this = $(this);
            // if the current path is like this link, make it active
            if ($this.attr('href') == current) {
                $this.parents('li').addClass('active');
                $this.parents('.treeview').addClass('active');
            }
        })

    </script>
</body>
</html>